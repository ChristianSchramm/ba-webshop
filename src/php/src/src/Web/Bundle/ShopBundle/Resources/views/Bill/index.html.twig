{% extends "::base.html.twig" %}

{% block title %}WebShopBundle:Bill:index{% endblock %}

{% block body %}
<h1>Welcome to the Bill:index page</h1>
<ul>
  {% for bill in bills %}
    <li>Rechnung vom {{ bill.date|date('d.m.Y') }} - {{ bill.rechnungsnummer }} <a target="_blank" href="/uploads/bills/{{ bill.path }}">Hier</a></li>
  {% endfor %}
</ul>
{% endblock %}
